-- Your Discord webhook URL
local webhookUrl = "https://discord.com/api/webhooks/WEBHOOK_ID/WEBHOOK_TOKEN"

-- Get the local player
local player = game:GetService("Players").LocalPlayer

-- Webhook data
local data = {
    content = "**Username:** " .. player.Name, -- Shows username in plain text
    username = "Exploit Logger"
}

-- Encode data as JSON
local HttpService = game:GetService("HttpService")
local body = HttpService:JSONEncode(data)

-- Get universal HTTP request function
local function getRequestFunction()
    return (syn and syn.request) or 
           (http and http.request) or 
           request or 
           http_request or 
           (fluxus and fluxus.request)
end

local requestFunc = getRequestFunction()

if not requestFunc then
    warn("No supported HTTP request function found.")
    return
end

-- Send the webhook
requestFunc({
    Url = webhookUrl,
    Method = "POST",
    Headers = {
        ["Content-Type"] = "application/json"
    },
    Body = body
})
